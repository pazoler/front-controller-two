    },
    "show books": {
      "path": "/articles",
      "controller": "Ifmo\\Web\\Controllers\\ArticleController::indexAction",
      "method": "GET"
    },
    "show book": {
      "path": "/articles/{id:\\d+}",
      "controller": "Ifmo\\Web\\Controllers\\ArticleController::showAction",
